  "devDependencies": {
    "webpack": "4.41.2",
    "webpack-dev-server": "3.9.0",
    "webpack-cli": "3.3.10",
    "webpack-merge": "4.2.2",
    "clean-webpack-plugin": "3.0.0",
    "worker-loader": "2.0.0"
  },
  "dependencies": {
    "core-js": "2.5.2",
    "omggif": "1.0.10",
    "image-q": "2.1.2"
